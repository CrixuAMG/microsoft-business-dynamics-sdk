{
    "name": "crixuamg/microsoft-business-dynamics-sdk",
    "version": "0.1.0",
    "description": "Saint Systems Microsoft Dynamics 365 SDK for PHP",
    "keywords": ["crm", "xrm", "dynamics", "dynamics crm", "dynamics 365", "sdk"],
    "homepage": "https://github.com/saintsystems/dynamics-sdk-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saint Systems",
            "email": "contact@saintsystems.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "guzzlehttp/guzzle": "^7.5",
        "monolog/monolog": "^2.0|^3.2",
        "nesbot/carbon": "^2.64",
        "illuminate/support": "^9.43"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
            "src/OData/Core/helpers.php"
        ],
        "psr-4": {
            "SaintSystems\\OData\\": "src/OData",
            "Microsoft\\Dynamics\\": "src/Dynamics"
        }
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
